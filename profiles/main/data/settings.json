{
  "editor.minimap.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.fontLigatures": true,
  "workbench.sideBar.location": "right",
  "console-ninja.featureSet": "Community",
  "files.associations": {
    ".env_*": "dotenv",
    ".env*": "dotenv"
  },
  "editor.tokenColorCustomizations": {
    "[Tokyo Night]": {
      "textMateRules": [
        {
          "scope": ["entity.name.tag"],
          "settings": { "foreground": "#ff6188" }
        },
        {
          "scope": [
            "entity.name.tag.js.jsx support.class.component.js.jsx",
            "entity.name.tag support.class.component",
            "source.vue support.class.component"
          ],
          "settings": { "foreground": "#00fab5" }
        }
      ]
    },
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    }
  },
  "dotenv.enableAutocloaking": false,
  "workbench.colorTheme": "Tokyo Night",
  "editor.fontFamily": "OperatorMono-Light, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "terminal.integrated.env.osx": {},
  "workbench.iconTheme": "vs-nomo-dark",
  "parameterHints.enabled": false,
